.mouseOver{
cursor: pointer;
}

.mouseOver:hover{

font-weight:bold;
}

.mouseOver:hover img{
    background-image:linear-gradient(to bottom, rgba(200, 255, 200, 1) 0%, rgba(200, 255, 200, 1) 100%), linear-gradient(to bottom, rgba(200, 255, 200, 1) 0%, rgba(200, 255, 200, 1) 100%);
    background-clip: content-box, padding-box;
    
}